{
  "name": "@outcome-co/verdaccio-github-auth",
  "version": "1.3.1",
  "description": "Verdaccio Github Authentication Plugin",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src -d dist --copy-files",
    "clean": "rm -rf dist coverage",
    "lint": "eslint --ignore-path .gitignore --max-warnings 0 ./src",
    "postbuild": "gulp postbuild",
    "prebuild": "gulp clean",
    "prepublishOnly": "yarn build",
    "pretest": "yarn typecheck",
    "secrets:clear": "node ./bin/secrets.js clear",
    "secrets:sync": "node ./bin/secrets.js sync",
    "server": "yarn build && nodemon --delay 1 --watch dist --signal SIGTERM ./run/server.js",
    "test": "yarn test:unit && yarn test:integration",
    "test:integration": "TEST_ENV=integration jest --passWithNoTests ./src",
    "test:unit": "TEST_ENV=test jest --passWithNoTests ./src",
    "typecheck": "tsc",
    "watch": "watch 'yarn build' src --wait=0.5",
    "watch:push": "watch 'yalc push --changed' src --wait=0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/outcome-co/verdaccio-github-auth-js"
  },
  "author": {
    "name": "Outcome Engineering",
    "email": "engineering@outcome.co",
    "url": "https://outcome.co"
  },
  "license": "ISC",
  "bugs": {
    "email": "engineering@outcome.co"
  },
  "homepage": "https://github.com/outcome-co/verdaccio-github-auth-js",
  "dependencies": {
    "@octokit/graphql": "^4.5.4",
    "@verdaccio/commons-api": "^9.7.1",
    "json-to-graphql-query": "^2.0.0",
    "limiter": "^1.1.5",
    "lodash": "^4.17.20",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@octokit/rest": "^18.0.3",
    "@outcome-co/devkit": "^8.1.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.6",
    "@verdaccio/types": "^9.7.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsdoc": "^30.7.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "gulp": "^4.0.2",
    "jest": "^26.6.3",
    "jest-retries": "^1.0.1",
    "mustache": "^4.0.1",
    "nodemon": "^2.0.4",
    "objectmodel": "^4.2.2",
    "promise-retry": "^2.0.1",
    "tweetsodium": "0.0.5",
    "typescript": "^4.0.5",
    "verdaccio": "^4.8.1",
    "yargs": "^16.0.3"
  },
  "engines": {
    "node": ">=10"
  },
  "optionalDependencies": {
    "@outcome-co/verdaccio-google-cloud": "^1.1.0"
  },
  "resolutions": {
    "chokidar": "^3.4.0",
    "node-forge": "^0.10.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
