{
  "name": "@outcome-co/verdaccio-github-auth",
  "version": "2.0.1",
  "description": "Verdaccio Github Authentication Plugin",
  "main": "./dist/index.js",
  "types": "./types/types.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "gulp build",
    "clean": "gulp clean",
    "lint": "yarn lint:prettier:check && yarn lint:eslint",
    "lint:prettier:check": "prettier --check ./src ./types/*d.ts",
    "lint:prettier": "prettier --write ./src ./types/*d.ts",
    "lint:eslint": "eslint --ignore-path .gitignore --max-warnings 0 ./src ./types",
    "prepublishOnly": "yarn build",
    "test": "yarn jest --color --passWithNoTests ./src",
    "test:integration": "TEST_ENV=integration jest --color --passWithNoTests ./src",
    "test:unit": "TEST_ENV=test jest --color --passWithNoTests ./src",
    "watch": "watch 'yarn build' src --wait=0.5",
    "watch:push": "watch 'yalc push --changed' src --wait=0.5",
    "secrets:clear": "ts-node ./bin/secrets.ts clear",
    "secrets:sync": "ts-node ./bin/secrets.ts sync",
    "fixtures:clear": "ts-node ./bin/reset.ts clear",
    "server": "yarn build && nodemon --delay 1 --watch dist --signal SIGTERM ./run/server.js",
    "graphql:codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/outcome-co/verdaccio-github-auth-js"
  },
  "author": {
    "name": "Outcome Engineering",
    "email": "engineering@outcome.co",
    "url": "https://outcome.co"
  },
  "license": "ISC",
  "bugs": {
    "email": "engineering@outcome.co"
  },
  "homepage": "https://github.com/outcome-co/verdaccio-github-auth-js",
  "dependencies": {
    "@octokit/graphql": "^4.5.4",
    "@verdaccio/commons-api": "^9.7.1",
    "json-to-graphql-query": "^2.0.0",
    "limiter": "^1.1.5",
    "lodash": "^4.17.20",
    "node-cache": "^5.1.2",
    "zod": "^2.0.0-beta.30"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-resolvers": "1.18.1",
    "@octokit/graphql-schema": "^10.13.0",
    "@octokit/rest": "^18.0.3",
    "@outcome-co/devkit": "^9.4.1",
    "@types/gulp": "^4.0.7",
    "@types/gulp-sourcemaps": "^0.0.34",
    "@types/jest": "^26.0.15",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.165",
    "@types/mustache": "^4.1.0",
    "@types/node": "^14.14.6",
    "@types/promise-retry": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "@verdaccio/types": "^9.7.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsdoc": "^31.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "graphql": "^15.4.0",
    "gulp": "^4.0.2",
    "jest": "^26.6.3",
    "listr": "^0.14.3",
    "mustache": "^4.0.1",
    "node-ts": "^5.1.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "promise-retry": "^2.0.1",
    "ts-jest": "^26.4.4",
    "tweetsodium": "0.0.5",
    "types-package-json": "^2.0.25",
    "typescript": "^4.0.5",
    "verdaccio": "^4.8.1",
    "watch": "^1.0.2",
    "yargs": "^16.0.3",
    "yesno": "^0.3.1"
  },
  "engines": {
    "node": ">=10"
  },
  "optionalDependencies": {
    "@outcome-co/verdaccio-google-cloud": "^1.1.0"
  },
  "resolutions": {
    "chokidar": "^3.4.0",
    "node-forge": "^0.10.0",
    "date-and-time": "^0.14.2",
    "dompurify": "^2.0.17"
  },
  "publishConfig": {
    "access": "public"
  }
}
